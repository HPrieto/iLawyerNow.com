<!DOCTYPE html>
<html>
<head>
	<title>The Law Club</title>
	<!-- Tab icon -->
	<link rel="icon" href="./lawyer_tab.png">
	<meta charset="utf-8">
	<!-- Mobile Phone Layout meta -->
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
	<!-- Bootstrap CSS CDN -->
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
	<!-- Ubuntu font CDN -->
	<link href="https://fonts.googleapis.com/css?family=Ubuntu:300,400,500,700" rel="stylesheet">
	<!-- Lato font CDN -->
    <link href="https://fonts.googleapis.com/css?family=Lato:100,300" rel="stylesheet">
    <style type="text/css" media="screen">
        body {
          background-color: #E1E1E0;
        }
    </style>
</head>
<body>
	<!-- React Code Starting div -->
	<div id='root'></div>
	<!-- Google Maps Initialization -->
	<script type='text/javascript'>
		var map;
		/* Initializes mapView object */
		function myMap() {
			// Map object specification
			var mapOptions = {
				center: new google.maps.LatLng(34.111603, -117.531065),
        		zoom: 12,
        		mapTypeId: 'roadmap'
			}
			// Set map to DOM element
			map = new google.maps.Map(document.getElementById("map"), mapOptions);
		}
	</script>
	<!-- webpack optimized package -->
    <script type='text/javascript' src='/build/bundle.js'></script>
	<!-- jQuery CDN -->
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
	<!-- Bootstrap CDN -->
	<script src='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js'></script>
	<!-- Google Maps JavaScript Library -->
	<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyB6Pfvnefo67yC-fNpJaWAC60Rb76_uVpw&callback=myMap"></script>
</body>
</html>